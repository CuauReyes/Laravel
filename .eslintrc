{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true
		}
	},
	"plugins": [
		"react"
	],
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"rules": {
		"no-debugger": 2,
		"semi": "error",
		"no-unused-vars": "off",
		"react/prop-types": "on"
	}
}
